generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Visitor {
  visitorId  String   @id // Mude de @unique para @id e remova o campo id
  userAgent  String?
  firstVisit DateTime @default(now())
  lastVisit  DateTime @default(now())
  visitCount Int      @default(1)
}
